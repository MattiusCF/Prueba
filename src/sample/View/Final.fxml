<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<AnchorPane prefHeight="480.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.InitializeController">
   <children>
      <ImageView fitHeight="480.0" fitWidth="400.0" pickOnBounds="false">
         <image>
            <Image url="@../resource/fondo.jpg" />
         </image>
      </ImageView>
      <Button style="-fx-background-color: white" fx:id="finish" layoutX="240.0" layoutY="400.0" mnemonicParsing="false" onAction="#finishGame" prefHeight="40.0" prefWidth="120.0">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="120.0" pickOnBounds="false" preserveRatio="false">
               <image>
                  <Image url="@../resource/Salir.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
