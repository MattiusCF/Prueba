<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="480.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.InitializeController">
   <children>
      <Label layoutX="80.0" layoutY="40.0" prefHeight="240.0" prefWidth="240.0" text="Lose!" />
      <Button fx:id="exit" layoutX="40.0" layoutY="416.0" mnemonicParsing="false" onAction="#surrender" prefHeight="40.0" prefWidth="120.0" text="Salir" />
      <Button fx:id="next" layoutX="240.0" layoutY="416.0" mnemonicParsing="false" onAction="#continueGame" prefHeight="40.0" prefWidth="120.0" text="Continuar" />
   </children>
</AnchorPane>
